#include <stdio.h>
#include <string.h>
int main(){
   int arr[9],max=0,flag;
   for(int i=0;i<9;i++){
      scanf("%d",&arr[i]);
   }
   for(int i=0;i<9;i++){
      if(max<arr[i]){
         max=arr[i];
         flag=i+1;
      }
   }
   printf("%d\n",max);
   printf("%d\n",flag);
   

   return 0;
}
